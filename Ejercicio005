public class Ejercicio005 {
    public static void main(String[] args) {
        // Inicializa la variable para almacenar la suma total
        int sumaTotal = 0;

        System.out.println("Números del 1 al 10:");

        // Bucle for para iterar desde 1 hasta 10
        for (int i = 1; i <= 10; i++) {
            System.out.println(i); // Imprime el número actual
            sumaTotal += i; // Suma el número actual a la suma total
        }

        // Imprime la suma total al finalizar el bucle
        System.out.println("La suma total de los números del 1 al 10 es: " + sumaTotal);
    }
}
